<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fudd.main">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.fudd.timetracker.activity.TimeTrackerActivity" android:label="@string/app_timetracker"/>
        <activity android:name="com.fudd.calendarstudy.activity.CalendarActivity" android:label="@string/app_calendarstudy"/>

        <service android:name="com.fudd.timetracker.service.TimeService"/>
    </application>

</manifest>